// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model code {
  id              Int      @id @default(autoincrement())
  matricule       String
  fullName        String
  country         String
  periodCode      String   @map("amci_country_code")
  scholarshipCode String   @map("scholarchip_code")
  period          String
  numPassport     String   @map("num_passport")
  createdAt       DateTime @default(now()) @map("created_at")

  @@unique([matricule, scholarshipCode], fullName: "matricule_scholarshipCode")
  @@index([matricule], fullName: "matricule")
  @@index([name], fullName: "fullName")
  @@index([scholarshipCode], fullName: "scholarshipCode")
}
